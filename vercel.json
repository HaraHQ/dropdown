{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "storybook-static/index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "storybook-static"
      }
    }
  ],
  "routes": [
    {
      "src": "/storybook(.*)",
      "dest": "storybook-static$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/$1"
    }
  ]
}